                    7,EVAASIEVAL7  )  AS NOTAS,
   EVAASIMEN
  from DATOS_BOLETIN,
       BOL_ESTUDIANTE est, 
       BOL_ASIGNATURA  asig
  where DABOLCONSEC= $P{CONSEC}
   AND ESTCONSECBOL= DABOLCONSEC
   AND  ASICONSECBOL = ESTCONSECBOL
   AND ASICODEST = ESTCODIGO
  and EVAASIMEN is not null
   ORDER BY GRACODIGO,
            GRUCODIGO,
