       EST_TIPO_DOC,
       TO_CHAR(SYSDATE,'YYYY/MM/DD') AS FECHA,
  --    UPPER(CONCAT(PerNombre1,CONCAT(PERNOMBRE2,CONCAT(' ',concat(PerApellido1,concat(' ',PerApellido2)))))) AS COORDINADOR
