FROM  REP_HOJA_VIDA
--LEFT OUTER JOIN GRUPO ON GRUCODJERAR = GRU_COD
--LEFT OUTER JOIN  PERSONAL ON  pernumdocum = grucodcoord
WHERE USUARIO =  $P{usuario}
 AND FECHA =  $P{fecha} 
ORDER BY INS_COD,
     SED_COD,
     JORNADA,
     GRA_COD,  
     GRUPO,
EST_APELL,       
EST_NOMB
