round(count(POA_PLANACTIVIDAD2.PLACCODIGO)*100/(select count(POA_PLANACTIVIDAD2.PLACCODIGO) from POA_PLANACTIVIDAD2 INNER JOIN POA ON POA.POACODINST = POA_PLANACTIVIDAD2.PLACCODINST INNER JOIN INSTITUCION ON INSTITUCION.INSCODIGO = POA.POACODINST where POA.POAESTADO=1  and POA.POAVIGENCIA = $P{VIGENCIA} AND (-99=$P{COD_LOCALIDAD} or INSTITUCION.INSCODLOCAL = $P{COD_LOCALIDAD})),1) as PORCENTAJE
from POA_PLANACTIVIDAD2 
INNER JOIN POA_AREAGESTION ON POA_AREAGESTION.ARGECODIGO = POA_PLANACTIVIDAD2.PLACCODARGESTION
INNER JOIN POA ON POA.POACODINST = POA_PLANACTIVIDAD2.PLACCODINST
INNER JOIN INSTITUCION ON INSTITUCION.INSCODIGO = POA.POACODINST
where POA.POAESTADO=1  and POA.POAVIGENCIA = $P{VIGENCIA}
AND (-99=$P{COD_LOCALIDAD} or INSTITUCION.INSCODLOCAL = $P{COD_LOCALIDAD})
group by POA_PLANACTIVIDAD2.PLACCODARGESTION,POA_AREAGESTION.ARGENOMBRE]]></queryString>

