		<parameter name="VIGENCIA" isForPrompting="false" class="java.lang.String"/>
		<parameter name="INSTITUCION" isForPrompting="false" class="java.lang.Long"/>
		<parameter name="COD_OBJETIVO" isForPrompting="false" class="java.lang.Long"/>
		<queryString><![CDATA[select B.ARGENOMBRE,B.ARGECODIGO,C.LIACNOMBRE, C.LIACCODIGO,A.PLACOBJETIVO,A.PLACNOMBRE,A.SEGPORCEJECP1,A.SEGPORCEJECP2,A.SEGPORCEJECP3,A.SEGPORCEJECP4,A.PLACCANTIDAD,A.PLACPONDERADO,A.SEGPERIODO1,A.SEGPERIODO2,A.SEGPERIODO3,A.SEGPERIODO4,
CASE A.PLACCODTIMETA
when 1 then 
to_number(to_number(nvl(A.SEGPERIODO1,0))+to_number(nvl(A.SEGPERIODO2,0))+to_number(nvl(A.SEGPERIODO3,0))+to_number(nvl(A.SEGPERIODO4,0)))
when 2 then 
to_number(to_number(nvl(A.SEGPERIODO1,0))+to_number(nvl(A.SEGPERIODO2,0))+to_number(nvl(A.SEGPERIODO3,0))+to_number(nvl(A.SEGPERIODO4,0)))
when 3 then 
(to_number(to_number(nvl(A.SEGPERIODO1,0))+to_number(nvl(A.SEGPERIODO2,0))+to_number(nvl(A.SEGPERIODO3,0))+to_number(nvl(A.SEGPERIODO4,0))))/CASE (CASE to_number(nvl(SEGPERIODO4,0)) WHEN 0 THEN 0 ELSE 1 END+CASE to_number(nvl(SEGPERIODO3,0)) WHEN 0 THEN 0 ELSE 1 END+CASE to_number(nvl(SEGPERIODO2,0)) WHEN 0 THEN 0 ELSE 1 END+CASE to_number(nvl(SEGPERIODO1,0)) WHEN 0 THEN 0 ELSE 1 END) WHEN 0 THEN 1 ELSE (CASE to_number(nvl(SEGPERIODO4,0)) WHEN 0 THEN 0 ELSE 1 END+CASE to_number(nvl(SEGPERIODO3,0)) WHEN 0 THEN 0 ELSE 1 END+CASE to_number(nvl(SEGPERIODO2,0)) WHEN 0 THEN 0 ELSE 1 END+CASE to_number(nvl(SEGPERIODO1,0)) WHEN 0 THEN 0 ELSE 1 END) END
end AVANCE_TOTAL,

CASE A.PLACCODTIMETA
when 1 then 
to_number(to_number(nvl(A.SEGPERIODO1,0))+to_number(nvl(A.SEGPERIODO2,0))+to_number(nvl(A.SEGPERIODO3,0))+to_number(nvl(A.SEGPERIODO4,0)))/A.PLACCANTIDAD
when 2 then 
NVL(CASE SEGPORCEJECP4 WHEN 0 THEN NULL ELSE SEGPORCEJECP4 END,
NVL(CASE SEGPORCEJECP3 WHEN 0 THEN NULL ELSE SEGPORCEJECP3 END,
NVL(CASE SEGPORCEJECP2 WHEN 0 THEN NULL ELSE SEGPORCEJECP2 END,
NVL(CASE SEGPORCEJECP1 WHEN 0 THEN NULL ELSE SEGPORCEJECP1 END
,0))))
when 3 then 
((to_number(to_number(nvl(A.SEGPERIODO1,0))+to_number(nvl(A.SEGPERIODO2,0))+to_number(nvl(A.SEGPERIODO3,0))+to_number(nvl(A.SEGPERIODO4,0))))/CASE (CASE SEGPORCEJECP4 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP3 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP2 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP1 WHEN 0 THEN 0 ELSE 1 END) WHEN 0 THEN 1 ELSE (CASE SEGPORCEJECP4 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP3 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP2 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP1 WHEN 0 THEN 0 ELSE 1 END) END)/A.PLACCANTIDAD
end PORC_EJECUCION,

CASE A.PLACCODTIMETA
when 1 then 
(to_number(to_number(nvl(A.SEGPERIODO1,0))+to_number(nvl(A.SEGPERIODO2,0))+to_number(nvl(A.SEGPERIODO3,0))+to_number(nvl(A.SEGPERIODO4,0)))/A.PLACCANTIDAD)*(A.PLACPONDERADO/100)
when 2 then 
NVL(CASE SEGPORCEJECP4 WHEN 0 THEN NULL ELSE SEGPORCEJECP4 END,
NVL(CASE SEGPORCEJECP3 WHEN 0 THEN NULL ELSE SEGPORCEJECP3 END,
NVL(CASE SEGPORCEJECP2 WHEN 0 THEN NULL ELSE SEGPORCEJECP2 END,
NVL(CASE SEGPORCEJECP1 WHEN 0 THEN NULL ELSE SEGPORCEJECP1 END
,0))))*(A.PLACPONDERADO/100)
when 3 then 
(((to_number(to_number(nvl(A.SEGPERIODO1,0))+to_number(nvl(A.SEGPERIODO2,0))+to_number(nvl(A.SEGPERIODO3,0))+to_number(nvl(A.SEGPERIODO4,0))))/CASE (CASE SEGPORCEJECP4 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP3 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP2 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP1 WHEN 0 THEN 0 ELSE 1 END) WHEN 0 THEN 1 ELSE (CASE SEGPORCEJECP4 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP3 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP2 WHEN 0 THEN 0 ELSE 1 END+CASE SEGPORCEJECP1 WHEN 0 THEN 0 ELSE 1 END) END)/A.PLACCANTIDAD)*(A.PLACPONDERADO/100)
end AVANCE_PONDERADOR,


NVL(CASE SEGFTEVERIFP4 WHEN null THEN NULL ELSE SEGFTEVERIFP4 END,
NVL(CASE SEGFTEVERIFP3 WHEN null THEN NULL ELSE SEGFTEVERIFP3 END,
NVL(CASE SEGFTEVERIFP2 WHEN null THEN NULL ELSE SEGFTEVERIFP2 END,
NVL(CASE SEGFTEVERIFP1 WHEN null THEN NULL ELSE SEGFTEVERIFP1 END
,'')))) as FTE_VERF,

NVL(CASE SEGANALISISP4 WHEN null THEN NULL ELSE SEGANALISISP4 END,
NVL(CASE SEGANALISISP3 WHEN null THEN NULL ELSE SEGANALISISP3 END,
NVL(CASE SEGANALISISP2 WHEN null THEN NULL ELSE SEGANALISISP2 END,
NVL(CASE SEGANALISISP1 WHEN null THEN NULL ELSE SEGANALISISP1 END
,'')))) as ANALISIS,
A.PLACFECHATERMIN,A.SEGFECHACUMPLIMT,
CASE 
when A.SEGFECHACUMPLIMT IS NULL 
then 
     CASE 
     WHEN A.PLACFECHATERMIN > sysdate
     THEN 'Aún queda tiempo para cumplir con la meta'
     ELSE 'La meta presenta retraso en su cumplimiento'     
     END 
else 
     CASE
     WHEN A.SEGFECHACUMPLIMT = A.PLACFECHATERMIN
     THEN 'La meta se cumplió en el tiempo establecido'
     WHEN A.SEGFECHACUMPLIMT < A.PLACFECHATERMIN
     THEN 'La meta se cumplió antes del tiempo establecido' 
     WHEN A.SEGFECHACUMPLIMT > A.PLACFECHATERMIN
     THEN 'La meta se cumplió después del tiempo establecido' 
     end
end ESTADO_ACTIVIDAD,
POA.POACODINST, A.PLACCODIGO, A.PLACCANTIDAD as CANTIDAD,(select U.UNMENOMBRE from POA_UNIMEDIDA U where U.UNMECODIGO = A.PLACCODUNMEDIDA) as UNIDAD_MEDIDA,    
      (  SELECT COALESCE(OBJNOMBRE,' ') 
        FROM POA_OBJETIVO 
        WHERE OBJCODIGO = PLACCODOBJETIVO
     ) AS OBJNOMBRE,
    to_number(trim($P{VIGENCIA})) AS VIGENCIA
FROM POA_PLANACTIVIDAD A
INNER JOIN POA_AREAGESTION B ON B.ARGECODIGO = A.PLACCODARGESTION
INNER JOIN POA_LINEAACCION C ON C.LIACCODIGO = A.PLACCODLIACCION
INNER JOIN POA ON POA.POACODINST = A.PLACCODINST and POA.POAVIGENCIA = A.PLACVIGENCIA
where POA.POAVIGENCIA = $P{VIGENCIA}
and POA.POACODINST = $P{INSTITUCION}
and POA.POAESTADO = 1
AND ( $P{VIGENCIA} < 2011  OR -99= $P{COD_OBJETIVO} OR PLACCODOBJETIVO = $P{COD_OBJETIVO})
order by B.ARGECODIGO, C.LIACCODIGO]]></queryString>

