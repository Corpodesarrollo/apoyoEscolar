INNER JOIN POA_AREAGESTION ON POA_AREAGESTION.ARGECODIGO =POA_PLANACTIVIDAD.PLACCODARGESTION
INNER JOIN POA_LINEAACCION ON POA_LINEAACCION.LIACCODIGO = POA_PLANACTIVIDAD.PLACCODLIACCION
