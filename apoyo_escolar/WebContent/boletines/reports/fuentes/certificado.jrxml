	<queryString>
		<![CDATA[select EST.ESTCODIGO,EST.NOMTIPODOC||'. '||EST.ESTNUMDOC as DOC, ESTNOMBRE1, EST.ESTNOMBRE2, EST.ESTAPELLIDO1, EST.ESTAPELLIDO2, EST.GRANOMBRE, EST.NOMDIRGRUPO,EST.EVACOMPOBS, ESTOBS, ESTOBSASIG, ESTOBSGRUPO, ESTOBSPER,ESTAUSENCIAS,GRUCODIGO,NIVNOMBRE,ESTAPROBO,
A.ARECODIGO, A.ARENOMBRE,A.EVAAREEVAL7,A.EVAAREMEN,A.AREIH,
M.ASICODIGO, M.ASINOMBRE,M.ASIIH,M.EVAASIEVAL7,M.EVAASIMEN,
TO_CHAR(sysdate,'dd')||' dias del mes '|| case TO_CHAR(sysdate,'mm') when '01' then 'Enero' when '02' then 'Febrero' when '03' then 'Marzo' when '04' then 'Abril' when '05' then 'Mayo' when '06' then 'Junio' when '07' then 'Julio' when '08' then 'Agosto' when '09' then 'Septiembre' when '10' then 'Octubre' when '11' then 'Noviembre' when '12' then 'Diciembre' end ||' de '|| TO_CHAR(sysdate,'yyyy') as fecha
from BOL_ASIGNATURA M
right JOIN BOL_AREA A on A.ARECODIGO=M.ASICODAREA and A.ARECODEST = M.ASICODEST and A.ARECONSECBOL=M.ASICONSECBOL
INNER JOIN BOL_ESTUDIANTE EST on EST.ESTCODIGO = A.ARECODEST and EST.ESTCONSECBOL = A.ARECONSECBOL
where EST.ESTCONSECBOL = $P{CONSECBOL}
order by GRUCODIGO,EST.ESTAPELLIDO1, EST.ESTAPELLIDO2, ESTNOMBRE1, EST.ESTNOMBRE2, ARECODIGO, ASICODIGO]]>
	</queryString>
